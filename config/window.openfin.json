{
  "$schema": "./openfin.json",
  "devtools_port": 9091,
  "startup_app": {
    "name": "AltSalesForceApp",
    "description": "A Salesforce Supporting Sample App to Listen To and Publish Messages",
    "url": "https://brmtx.csb.app/app.html",
    "uuid": "AltAppSalesForce",
    "autoShow": true,
    "defaultHeight": 950,
    "defaultWidth": 800,
    "api": {
      "iframe": {
        "crossOriginInjection": true,
        "sameOriginInjection": true
      }
    }
  },
  "runtime": {
    "arguments": "--v=1 --inspect",
    "version": "stable"
  },
  "shortcut": {
    "company": "OpenFin",
    "description": "SalesForce Container",
    "icon": "https://brmtx.csb.app/favicon.ico",
    "name": "OpenFin SalesForce Support App"
  }
}
